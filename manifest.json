{
  "manifest_version": 2,

  "name": "Github line counter",
  "description": "This extension counting number of lines",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["myscript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://github.com/*", "tabs", "webNavigation"
  ]
}
